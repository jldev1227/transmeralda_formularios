{
  "expo": {
    "scheme": "transmeralda",
    "name": "preoperacional",
    "slug": "preoperacional",
    "version": "1.0.0",
    "backgroundColor": "#fff",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.tuempresa.transmeralda",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "9aaa0e0f-39f2-455a-b6aa-45bd077704c4"
      }
    }
  }
}
